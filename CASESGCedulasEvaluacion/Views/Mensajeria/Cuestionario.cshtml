                            axios.post("/fumigacion/eliminaArchivo", { Id: id, Folio: model.folio }).then(response => {
                                if (response.status == 200) {
                                    Swal.fire({
                                        'icon': 'success',
                                        'title': 'Servicio de Mensajería Acelerada',
                                        'html': 'El archivo fue eliminado con éxito.',
                                    }).then(function () {
                                        axios.get("/fumigacion/getEntregables/" + model.id + "/" + model.servicioId).then(response => {
                                            if (response.data == null || response.data == "") {
                                                $('#tEntregables').html(response.data);
                                            }
                                            $('#tEntregables').html(response.data);
                                        });
                                    });
                                }
                            }).catch(error => {
                                Swal.fire({
                                    'icon': 'error',
                                    'title': 'Servicio de Mensajería Acelerada',
                                    'html': 'No se pudo procesar tu solicitud. Contacta a tu Administrador.',
                                });
                            });
                        }
                    });
                });

                $(document).on('click', '.view_file', function () {
                    let nombre = $(this).data('file');
                    let tipo = $(this).data('tipo');
                    let data = "<object width='100%' height='710px' data='/view/entregable/" + model.folio + "/" + nombre + "'></object>";
                    $('#titlePDF').text("PDF - \"" + tipo + "\"");
                    $('#objectPdf').html(data);
                    $('#show_pdf').modal('show');
                });
            /********************************* Fin Entregables ************************************/


            /********************************** Funciones Adicionales *********************************/
            function cuentaincidenciasMensajeria(pregunta) {
                var isContinue = true;
                $.ajax({
                    url: '/mensajeria/totalIncidencia/' + model.id + '/' + pregunta,
                    type: 'GET',
                    async: false,
                    success: function (json) {
                        json == 0 ? isContinue = false : true;
                    },
                });
                return isContinue;
            }

            /********************************** FIN Funciones Asincronas*********************************/

            /************************************ GUARDAR/ENVIAR Cédula ************************************/
            /*Guardar la Cedula*/
            $('.btn_guardar').click(function () {
                var respuestas = new Array();
                for (let i = 0; i < model.preguntas.length; i++) {
                    let respuesta = $("input[name=pregunta_" + (i + 1) + "]:checked").val();
                    detalles = null;
                    if (respuesta != null) {
                        if (respuesta == "N/A") {
                            detalles = "N/A";
                        } else if (model.preguntas[i].numero) {
                            if (respuesta == "false") {
                                detalles = $("#diasAtraso").val();
                            }
                        }
                    }
                    respuestas.push({ CedulaEvaluacionId: parseInt(model.id), Pregunta: (i + 1), Respuesta: (respuesta == "true"), Detalles: detalles });
                }

                axios.post("/mensajeria/evaluation", respuestas).then(response => {
                    if (response.status == 200) {
                        Swal.fire({
                            'icon': "success",
                            'title': 'Servicio de Mensajería Acelerada',
                            'html': "Se guardaron las respuesta de la cédula correctamente."
                        });
                    } else {
                        Swal.fire({
                            'icon': "error",
                            'title': 'Servicio de Mensajería Acelerada',
                            'html': "Hubo un error al guardar las respuestas de la cédula. Contacte al Administrador del Sistema"
                        });
                    }
                });
            });
            /*FIN de guardar la cedula*/

            /*Guardar la Cedula sin mensaje*/
            function guardaCedula() {
                var respuestas = new Array();
                for (let i = 0; i < model.preguntas.length; i++) {
                    let respuesta = $("input[name=pregunta_" + (i + 1) + "]:checked").val();
                    detalles = null;
                    if (respuesta != null) {
                        if (respuesta == "N/A") {
                            detalles = "N/A";
                        } else if (model.preguntas[i].numero) {
                            if (respuesta == "false") {
                                detalles = $("#diasAtraso").val();
                            }
                        }
                    }
                    respuestas.push({ CedulaEvaluacionId: parseInt(model.id), Pregunta: (i + 1), Respuesta: (respuesta == "true"), Detalles: detalles });
                }

                axios.post("/mensajeria/evaluation", respuestas).then(response => {
                    if (response.status == 200) {

                    } else {
                        Swal.fire({
                            'icon': "error",
                            'title': "Hubo un error al guardar las respuestas de la cédula. Contacte al Administrador del Sistema"
                        });
                    }
                });
            }
            /*FIN de guardar la cedula*/

            /*Validamos que ya fueron adjuntados todos los entregables*/
            function validaEntregables() {
                var msgEntregables = ["Factura", "Validación del SAT"];
                var entregables = ["Factura", "SAT"];
                var msg = "";
                var nota = false;
                var found = false;
                axios.get('/mensajeria/getListadoEntregables/' + model.id + "/" + model.servicioId).then(response => {
                    for (var j = 0; j < entregables.length; j++) {
                        for (var i = 0; i < response.data.length; i++) {
                            if (entregables[j] == response.data[i].tipo) {
                                found = true;
                                break;
                            } else {
                                found = false;
                            }
                        }
                        if (!found) {
                            msg += "<strong> - " + msgEntregables[j] + "<br/></strong>";
                        }
                    }

                    if (msg != "") {
                        Swal.fire({
                            'icon': 'error',
                            'title': 'Servicio de Mensajería Acelerada',
                            'html': 'Para envíar la cédula a evaluación, es necesario adjuntar los siguientes entregables: <br/><br/>' + msg
                        });
                        return false;
                    } else {
                        $('.btn_enviar').attr('disabled', true);
                        $('.btn_guardar').attr('disabled', true);
                        axios.post('/mensajeria/historialMensajeria', {
                            ServicioId: model.servicioId,
                            CedulaId: model.id, UsuarioId: parseInt(@User.Claims.ElementAt(0).Value),
                            Estatus: "Enviado a DAS", Comentarios: "Se envía la cédula a la DAS para su revisión."
                        }).then(response => {
                            axios.post("/mensajeria/sendCedula/" + model.servicioId + "/" + model.id).then(response => {
                                if (response.status == 200) {
                                    Swal.fire({
                                        'icon': "success",
                                        'title': 'Servicio de Mensajería Acelerada',
                                        'html': "Se envíó la cédula para su revisión correctamente."
                                    }).then(function () {
                                        window.location.href = "/mensajeria/index/" + model.servicioId;
                                    });
                                } else {
                                    Swal.fire({
                                        'icon': "error",
                                        'title': 'Servicio de Mensajería Acelerada',
                                        'html': "Hubo un error al guardar las respuestas de la cédula. Contacte al Administrador del Sistema"
                                    });
                                }
                            });
                        });
                    }
                });
            }

            /*FIN Validamos que ya fueron adjuntados todos los entregables*/

             function buscaPregunta(pregunta) {
                    let respuesta = $("input[name=pregunta_" + pregunta + "]:checked").val();
                    let exists = -1;

                    if (pregunta == 1 && model.preguntas[pregunta - 1].cierre)
                    {
                        return pregunta;
                    }
                    else
                    {
                        for (let i = 0; i < model.respuestasEncuesta.length; i++) {
                            if (pregunta == model.respuestasEncuesta[i].pregunta) {
                                exists = 1;
                            }
                        }

                        if (exists == 1 && respuesta != null && respuesta != undefined && respuesta != "") {
                            return model.preguntas[pregunta - 1].noPregunta;
                        }
                    }

                    return exists;
                }

             function muestraMensaje(pregunta){
                    for (var i = 0; i < model.preguntas.length; i++) {
                        if (model.preguntas[i].noPregunta == pregunta) {
                            if (model.preguntas[i].incidencias) {
                                Swal.fire({
                                    'icon': 'error',
                                    'title': 'Servicio de Mensajería Acelerada',
                                    'html': 'No ha capturado las <strong>incidencias</strong> en la <strong>Pregunta '+pregunta+'.</strong>'
                                });
                            }else if (model.preguntas[i].numero) {
                                Swal.fire({
                                    'icon': 'error',
                                    'title': 'Servicio de Mensajería Acelerada',
                                    'html': 'No ha capturado los <strong>Días Naturales de Atraso</strong> o los <strong>Días Naturales de Atraso son iguales a cero</strong> en la <strong>Pregunta '+pregunta+'.</strong>'
                                });
                            }
                            else
                            {
                                 Swal.fire({
                                     'icon': 'error',
                                     'title': 'Servicio de Mensajería Acelerada',
                                     'html': 'No ha capturado la <strong>Pregunta ' + model.preguntas[i].noPreguntaUsuario + '</strong>.'
                                 });
                            }
                        }
                    }
            }

             $('.btn_enviar').click(function () {
                    guardaCedula();
                    let pregunta = 0;
                    let i = 0;
                    let totalP = (perfil == false ? model.preguntas.length : (model.preguntas.length - 1));
                    let respuesta = "";
                    let isContinue = true;
                    if (model.facturas.length == 0) {
                        Swal.fire({
                            'icon': 'error',
                            'title': 'Servicio de Mensajería Acelerada',
                            'html': 'No ha registrado ninguna factura en la cédula'
                        });
                        return false;
                    }

                    for (i = 0; i < totalP; i++) {
                        respuesta = $('input:radio[name="pregunta_' + (i + 1) + '"]:checked').val();
                        if (buscaPregunta(model.preguntas[i].noPreguntaUsuario) != -1) {
                            pregunta = buscaPregunta(model.preguntas[i].noPreguntaUsuario);
                            if (respuesta == "true") {
                                if (pregunta == 4 || pregunta == 5 || pregunta == 6) {
                                    if (model.preguntas[i].incidencias) {
                                        isContinue = cuentaincidenciasMensajeria(model.preguntas[i].noPregunta);
                                        if (isContinue == false) {
                                            break;
                                        }
                                    }
                                }
                            }
                            else if (respuesta == "false") {
                                if (pregunta == 1 || pregunta == 2 || pregunta == 3) {
                                    if (model.preguntas[i].incidencias) {
                                        isContinue = cuentaincidenciasMensajeria(model.preguntas[i].noPregunta);
                                        if (isContinue == false) {
                                            break;
                                        }
                                    }
                                } else if(pregunta == 7) {
                                    if ($("#diasAtraso").val() == "" || $("#diasAtraso").val() == 0) {
                                        break;
                                    }
                                }
                            } 
                        } else {
                            pregunta = model.preguntas[i].noPregunta;
                            break;
                        }
                    }

                    if (i < totalP) {
                        muestraMensaje(pregunta)
                        return false;
                    } else {
                        validaEntregables();
                    }
                });
            /********************************** FIN GUARDAR/ENVIAR CÉDULA **********************************/
        });
    </script>
}

