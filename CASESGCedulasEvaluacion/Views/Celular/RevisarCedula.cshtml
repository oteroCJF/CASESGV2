                                            <li class="strong mt-3"><span class="fa-li"><i class="fad fa-star"></i></span>  <strong>Calificación: </strong> </b> @Model.Calificacion</li>
                                            <li class="strong mt-3"><span class="fa-li"><i class="fas fa-lg fa-user"></i></span><strong>Elaboró: </strong> @Model.usuarios.nombre_emp @Model.usuarios.paterno_emp @Model.usuarios.materno_emp</li>
                                            <li class="strong mt-3">
                                                <span class="fa-li"><i class="fas fa-lg fa-envelope mb-1"></i></span>

                                                @if (@Model.usuarios.correo_electronico.Equals("") || @Model.usuarios.correo_electronico.Equals(null))
                                                {
                                                    <strong>Correo:</strong><strong class="text-danger">@String.Format("Sin correo")</strong>
                                                }
                                                else
                                                {
                                                    <strong>Correo:</strong> @Model.usuarios.correo_electronico
                                                }

                                            </li>
                                        </ul>
                                    </div>
                                    <div class="col-lg-5">
                                        <div class="row col-lg-12">
                                            @if (totalP != 0)
                                            {
                                                <h5 class="@(@totalP == 0 ? "text-success":"text-danger") font-weight-bold">Penalizaciones: @String.Format(System.Globalization.CultureInfo.CurrentCulture, "{0:C}", totalP)</h5>
                                                <div class="col-lg-12">
                                                    <span class="fa-li"><i class="fas fa-tools"></i></span>
                                                    <b>Penalización: </b> <b class="text-primary">@String.Format(System.Globalization.CultureInfo.CurrentCulture, "{0:C}", @totalP)</b>
                                                </div>
                                            }
                                            else
                                            {
                                                <div class="col-lg-12 text-success font-weight-bold">
                                                    <h5>No hay penalizaciones</h5>
                                                </div>
                                            }
                                        </div>
                                        <div class="row col-lg-12 mt-4">
                                            @if (totalD != 0)
                                            {
                                                <div class="col-lg-12 @(totalD == 0 ? "text-pagado" : "text-primary")">
                                                    <h5 class="@(@totalD == 0 ? "text-pagado" : "text-danger") font-weight-bold">Deductivas: @String.Format(System.Globalization.CultureInfo.CurrentCulture, "{0:C}", totalD)</h5>
                                                    <p class="text-justify font-weight-bold">Solicitar al prestador de servicios la nota de crédito por el(los) siguiente(s) concepto(s):</p>
                                                </div>
                                                <div class="col-lg-12 ml-5">
                                                    <div class="col-lg-12 mt-2">
                                                        <span class="fa-li"><i class="fas fa-tools"></i></span>
                                                        <b>Deductiva: </b> <b class="text-primary">@String.Format(System.Globalization.CultureInfo.CurrentCulture, "{0:C}", @totalD)</b>
                                                    </div>
                                                </div>
                                            }
