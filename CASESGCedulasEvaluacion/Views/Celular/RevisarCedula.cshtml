                                                    <h5>No hay deductivas</h5>
                                                </div>
                                            }
                                        </div>
                                    </div>
                                    <div class="col-lg-2">
                                        <h4 class="@(Model.Fondo.Replace("bg", "text")) lblEstatus">
                                            @Html.Raw(Model.Icono)
