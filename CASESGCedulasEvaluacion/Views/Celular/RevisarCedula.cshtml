                                                @{ var i = 0;}
                                                @foreach (var p1 in Model.incidencias.altaEntrega)
                                                {
                                                    i = i + 1;
                                                    <tr>
                                                        <td>@i</td>
                                                        <td>Alta/Equipo</td>
                                                        <td>@p1.Linea</td>
                                                        <td>@p1.FechaSolicitud.ToString("dd/MM/yyyy hh:mm tt")</td>
                                                        <td>@p1.FechaAtencion.ToString("dd/MM/yyyy hh:mm tt")</td>
                                                        <td>@p1.HorasAtencion</td>
                                                        <td>@p1.HorasRetraso</td>
                                                        @if (@p1.HorasAtencion <= 24)
                                                        {
                                                            <td class="text-center">
                                                                <div class="icon">
                                                                    <i class="fas fa-times-circle text-danger"></i>
                                                                </div>
                                                            </td>
                                                        }
                                                        else
                                                        {
                                                            <td class="text-center">
                                                                <div class="icon">
                                                                    <i class="fas fa-check-circle text-success"></i>
                                                                </div>
