                        if (fechaProgramada == '') {
                            $('#fechaProgramada').addClass('is-invalid');
                            $('#error_fechaProgramada').css('display', 'block');
                        }
                        if (horaProgramada == '') {
                            $('#horaProgramada').addClass('is-invalid');
                            $('#error_horaProgramada').css('display', 'block');
                        }
                        if (horaRealizada == '') {
                            $('#horaRealizada').addClass('is-invalid');
