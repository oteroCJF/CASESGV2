                                                    Swal.fire({
                                                        'icon': 'error',
                                                        'title': 'Servicio de Suministro de Agua Purificada para Beber',
                                                        'text': 'Debe escribir correctamente la palabra ELIMINAR en mayúsculas.',
                                                    });
                                                    $('input:radio[name="pregunta_' + pusuario + '"][value="false"]').prop('checked', true);
                                                    $("#divIncidencias_" + pusuario).css("display", "block");
                                                }
                                            });
                                        } else {
                                            $('input:radio[name="pregunta_' + pusuario + '"][value="false"]').prop('checked', true);
                                            $("#divIncidencias_" + pusuario).css("display", "block");
                                        }
                                    });
                                }
                            });
                            $("#divIncidencias_" + pusuario).css("display", "none");
                        }
                    }
                });
            /********************************* Validaciones para las respuestas *******************************/

            /******************************* Incidencias Fumigación ***************************************/
                function muestraInputs(pregunta) {
                    $('#divCategoria').css('display', 'none');
                    $('#divFechaProgramada').css('display', 'none');
                    $('#divFechaRealizada').css('display', 'none');
                    $('#divHoraProgramada').css('display', 'none');
                    $('#divHoraRealizada').css('display', 'none');
                    $('#divCaracGarrafones').css('display', 'none');
                    $('#divInciComentarios').css('display', 'none');
                    $('#error_fechaProgramada').css('display', 'none');
                    $('#error_fechaRealizada').css('display', 'none');
                    $('#error_horaProgramada').css('display', 'none');
                    $('#error_horaRealizada').css('display', 'none');
                    $('#error_caracGarrafones').css('display', 'none');
                    $('#error_inciComentarios').css('display', 'none');

                    if (pregunta == 2) {
                        $("#categoria").val('Fechas');
