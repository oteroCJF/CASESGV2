                            axios.get('/agua/totalIncidencia/' + model.id + '/' + pregunta).then(total => {
                                if (total.data > 0) {
                                    Swal.fire({
                                        'icon': 'warning',
                                        'title': 'Servicio de Suministro de Agua Purificada para Beber',
